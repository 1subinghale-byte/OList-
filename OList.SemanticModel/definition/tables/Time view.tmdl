table 'Time view'
	lineageTag: f3555b74-4807-4f27-9ebf-3ac06a28875c

	calculationGroup
		precedence: 3

		calculationItem YTD = IF(NOT(ISBLANK(SELECTEDMEASURE())),CALCULATE(TOTALYTD(SELECTEDMEASURE(),'Calendar'[Date])),BLANK())

		calculationItem CY = SELECTEDMEASURE()

		calculationItem PY = ```
				
				IF(
				    NOT(ISBLANK(SELECTEDMEASURE())),
				        COALESCE(CALCULATE(SELECTEDMEASURE(),SAMEPERIODLASTYEAR('Calendar'[Date])),0)
				)
				
				```

		calculationItem Variance =
				SELECTEDMEASURE()-
				IF(NOT(ISBLANK(selectedmeasure())),
				    CALCULATE(SELECTEDMEASURE(),SAMEPERIODLASTYEAR('Calendar'[Date]))
				)

